<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NineGridsLayout">
        <attr name="childSpace" format="dimension"/>
        <attr name="selectedScale" format="float"/>
    </declare-styleable>
    <declare-styleable name="PageIndicatorView">
        <attr name="color" format="color"/>
    </declare-styleable>
    <declare-styleable name="ContentLoadingProgressBar">
        <attr name="loadingColor" format="color"/>
        <attr name="progressBarWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="TileGroup_Layout">
        <attr name="tileSpanX" format="integer" />
        <attr name="tileSpanY" format="integer"/>
        <attr name="tileIndexX" format="integer"/>
        <attr name="tileIndexY" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TileGroup">
        <attr name="tileSpanXCount" format="integer"/>
        <attr name="tileSpanYCount" format="integer"/>
        <attr name="tileSpaceWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RoundTileView">
        <attr name="tileCorner" format="dimension"/>
        <attr name="tileCornerLeftTop" format="dimension"/>
        <attr name="tileCornerRightTop" format="dimension"/>
        <attr name="tileCornerRightBottom" format="dimension"/>
        <attr name="tileCornerLeftBottom" format="dimension"/>
        <attr name="onlyClipChild" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="DashboardCardView">
        <attr name="markTextSize" format="dimension"/>
        <attr name="markTextColor" format="color"/>
        <attr name="markBackgroundColor" format="color"/>
        <attr name="markTextValue" format="string"/>
        <attr name="markPadding" format="dimension"/>
        <attr name="markPaddingLeft" format="dimension"/>
        <attr name="markPaddingTop" format="dimension"/>
        <attr name="markPaddingRight" format="dimension"/>
        <attr name="markPaddingBottom" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="VerticalProgressBar">
        <attr name="vpBackgroundColor" format="color"/>
        <attr name="vpProgressColor" format="color"/>
        <attr name="max" format="integer"/>
        <attr name="progress" format="integer"/>
    </declare-styleable>
</resources>